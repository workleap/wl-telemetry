{
    "name": "@workleap/logrocket",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "A package containing utilities for LogRocket.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-tracking.git",
        "directory": "packages/logrocket"
    },
    "publishConfig": {
        "access": "public",
        "provenance": true
    },
    "type": "module",
    "main": "./dist/index.js",
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        }
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "devDependencies": {
        "@rsbuild/core": "1.4.2",
        "@rslib/core": "0.10.4",
        "@workleap/eslint-plugin": "3.4.2",
        "@workleap/rslib-configs": "1.0.6",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.1",
        "happy-dom": "18.0.1",
        "typescript": "5.8.3",
        "vitest": "3.2.4"
    },
    "sideEffects": false
}
