{
    "name": "@workleap/mixpanel",
    "author": "Workleap",
    "version": "2.0.1",
    "description": "A package containing utilities for Mixpanel telemetry.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/mixpanel"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/js/index.d.ts",
                "import": "./dist/js/index.js",
                "default": "./dist/js/index.js"
            },
            "./react": {
                "types": "./dist/react/index.d.ts",
                "import": "./dist/react/index.js",
                "default": "./dist/react/index.js"
            },
            "./mocks": {
                "types": "./dist/mocks/index.d.ts",
                "import": "./dist/mocks/index.js",
                "default": "./dist/mocks/index.js"
            },
            "./msw": {
                "types": "./dist/msw/index.d.ts",
                "import": "./dist/msw/index.js",
                "default": "./dist/msw/index.js"
            }
        }
    },
    "exports": {
        ".": "./src/js/index.ts",
        "./react": "./src/react/index.ts",
        "./mocks": "./src/mocks/index.ts",
        "./msw": "./src/msw/index.ts"
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "@workleap/telemetry": "workspace:*",
        "msw": "^2.0.0",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        },
        "msw": {
            "optional": true
        }
    },
    "devDependencies": {
        "@rsbuild/core": "1.4.6",
        "@rslib/core": "0.10.5",
        "@types/react": "19.1.8",
        "@workleap/eslint-plugin": "3.4.2",
        "@workleap/rslib-configs": "1.0.6",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.1",
        "happy-dom": "18.0.1",
        "msw": "^2.10.2",
        "typescript": "5.8.3",
        "vitest": "3.2.4"
    },
    "sideEffects": false
}
