{
    "name": "@workleap/honeycomb",
    "author": "Workleap",
    "version": "7.1.8",
    "description": "Shared Honeycomb configuration for web application at Workleap.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/honeycomb"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/js/index.d.ts",
                "import": "./dist/js/index.js",
                "default": "./dist/js/index.js"
            },
            "./react": {
                "types": "./dist/react/index.d.ts",
                "import": "./dist/react/index.js",
                "default": "./dist/react/index.js"
            }
        }
    },
    "exports": {
        ".": "./src/js/index.ts",
        "./react": "./src/react/index.ts"
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "@opentelemetry/api": "^1.9.0",
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "dependencies": {
        "@honeycombio/opentelemetry-web": "^1.3.0",
        "@opentelemetry/auto-instrumentations-web": "^0.54.0",
        "@opentelemetry/instrumentation": "^0.209.0",
        "@opentelemetry/instrumentation-document-load": "^0.54.0",
        "@opentelemetry/instrumentation-fetch": "^0.209.0",
        "@opentelemetry/instrumentation-user-interaction": "^0.53.0",
        "@opentelemetry/instrumentation-xml-http-request": "^0.209.0",
        "@opentelemetry/sdk-trace-web": "^2.3.0",
        "@workleap-telemetry/core": "workspace:*",
        "@workleap/logging": "^1.3.3"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.7.2",
        "@rslib/core": "0.19.2",
        "@types/react": "19.2.8",
        "@typescript-eslint/parser": "8.53.0",
        "@workleap/eslint-configs": "1.1.9",
        "@workleap/rslib-configs": "1.1.5",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.2",
        "happy-dom": "20.1.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.53.0",
        "vitest": "4.0.17"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
