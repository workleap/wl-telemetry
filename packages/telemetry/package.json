{
    "name": "@workleap/telemetry",
    "author": "Workleap",
    "version": "2.0.1",
    "description": "A telemetry library for Workleap's web applications.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/telemetry"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/js/index.d.ts",
                "import": "./dist/js/index.js",
                "default": "./dist/js/index.js"
            },
            "./react": {
                "types": "./dist/react.d.ts",
                "import": "./dist/react.js",
                "default": "./dist/react.js"
            }
        }
    },
    "exports": {
        ".": "./src/js/index.ts",
        "./react": "./src/react/index.ts"
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "dependencies": {
        "@opentelemetry/sdk-trace-web": "^2.1.0",
        "@workleap-telemetry/core": "workspace:*",
        "@workleap/honeycomb": "workspace:*",
        "@workleap/logging": "^1.3.0",
        "@workleap/logrocket": "workspace:*",
        "@workleap/mixpanel": "workspace:*"
    },
    "devDependencies": {
        "@rsbuild/core": "1.5.13",
        "@rslib/core": "0.15.0",
        "@types/js-cookie": "3.0.6",
        "@types/react": "19.2.0",
        "@workleap/eslint-plugin": "3.5.0",
        "@workleap/rslib-configs": "1.1.0",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.1",
        "typescript": "5.9.3"
    },
    "sideEffects": false
}
