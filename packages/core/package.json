{
    "name": "@workleap-telemetry/core",
    "author": "Workleap",
    "version": "0.0.1",
    "description": "A package including telemetry core utilities for Workleap web applications.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/core"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "dependencies": {
        "@workleap/logging": "^1.3.0"
    },
    "devDependencies": {
        "@rsbuild/core": "1.5.11",
        "@rslib/core": "0.13.3",
        "@workleap/eslint-plugin": "3.5.0",
        "@workleap/rslib-configs": "1.1.0",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.1",
        "typescript": "5.9.2",
        "vitest": "3.2.4"
    },
    "sideEffects": false
}
