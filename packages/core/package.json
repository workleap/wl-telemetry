{
    "name": "@workleap-telemetry/core",
    "author": "Workleap",
    "version": "1.0.8",
    "description": "A package including telemetry core utilities for Workleap web applications.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/core"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsgo",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "dependencies": {
        "@workleap/logging": "^1.3.6",
        "js-cookie": "^3.0.5",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.2",
        "@rsbuild/core": "1.7.3",
        "@rslib/core": "0.19.5",
        "@types/js-cookie": "3.0.6",
        "@typescript-eslint/parser": "8.55.0",
        "@typescript/native-preview": "7.0.0-dev.20260209.1",
        "@workleap/eslint-configs": "1.1.12",
        "@workleap/rslib-configs": "1.1.7",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.2",
        "happy-dom": "20.6.0",
        "typescript": "5.9.3",
        "typescript-eslint": "8.55.0",
        "vitest": "4.0.18"
    },
    "sideEffects": false
}
