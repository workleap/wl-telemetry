{
    "name": "@workleap-telemetry/core",
    "author": "Workleap",
    "version": "1.0.5",
    "description": "A package including telemetry core utilities for Workleap web applications.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/core"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/index.d.ts",
                "import": "./dist/index.js",
                "default": "./dist/index.js"
            }
        }
    },
    "exports": "./src/index.ts",
    "files": [
        "src",
        "dist",
        "CHANGELOG.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "dependencies": {
        "@workleap/logging": "^1.3.2",
        "js-cookie": "^3.0.5",
        "uuid": "^13.0.0"
    },
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@rsbuild/core": "1.6.14",
        "@rslib/core": "0.18.4",
        "@types/js-cookie": "3.0.6",
        "@typescript-eslint/parser": "8.49.0",
        "@workleap/eslint-configs": "1.1.7",
        "@workleap/rslib-configs": "1.1.4",
        "@workleap/typescript-configs": "3.0.7",
        "eslint": "9.39.1",
        "happy-dom": "20.0.11",
        "typescript": "5.9.3",
        "typescript-eslint": "8.49.0",
        "vitest": "4.0.15"
    },
    "sideEffects": false
}
