{
    "name": "@workleap/common-room",
    "author": "Workleap",
    "version": "2.0.4",
    "description": "Shared Common Room utilities for web application at Workleap.",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/workleap/wl-telemetry.git",
        "directory": "packages/common-room"
    },
    "type": "module",
    "publishConfig": {
        "access": "public",
        "provenance": true,
        "exports": {
            ".": {
                "types": "./dist/js/index.d.ts",
                "import": "./dist/js/index.js",
                "default": "./dist/js/index.js"
            },
            "./react": {
                "types": "./dist/react/index.d.ts",
                "import": "./dist/react/index.js",
                "default": "./dist/react/index.js"
            }
        }
    },
    "exports": {
        ".": "./src/js/index.ts",
        "./react": "./src/react/index.ts"
    },
    "files": [
        "src",
        "dist",
        "CHANGELOG.md",
        "README.md"
    ],
    "scripts": {
        "build": "rslib build --config ./rslib.build.ts",
        "eslint": "eslint . --max-warnings=-0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc",
        "test": "vitest --config vitest.config.ts --no-watch"
    },
    "peerDependencies": {
        "react": "^18.0.0 || ^19.0.0",
        "react-dom": "^18.0.0 || ^19.0.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "react-dom": {
            "optional": true
        }
    },
    "dependencies": {
        "@workleap-telemetry/core": "workspace:*",
        "@workleap/logging": "^1.3.0"
    },
    "devDependencies": {
        "@rsbuild/core": "1.5.13",
        "@rslib/core": "0.15.0",
        "@types/react": "19.2.0",
        "@typescript-eslint/parser": "8.45.0",
        "@workleap/eslint-plugin": "3.5.0",
        "@workleap/rslib-configs": "1.1.0",
        "@workleap/typescript-configs": "3.0.4",
        "eslint": "8.57.1",
        "happy-dom": "19.0.2",
        "typescript": "5.9.3",
        "vitest": "3.2.4"
    },
    "sideEffects": false,
    "engines": {
        "node": ">=21.1.0"
    }
}
