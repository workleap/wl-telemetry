{
    "name": "@honeycomb-proxy/app",
    "author": "Workleap",
    "version": "0.0.0",
    "description": "Sample application to tests the Honeycomb library with a setup using a proxy to send the traces to Honeycomb.",
    "private": true,
    "license": "Apache-2.0",
    "type": "module",
    "scripts": {
        "dev": "rsbuild dev --config rsbuild.dev.ts",
        "build": "pnpm build:rsbuild && pnpm build:copy-public-files",
        "build:rsbuild": "rsbuild build --config rsbuild.build.ts",
        "build:copy-public-files": "copyfiles -u 1 public/favicon.png dist",
        "serve-build": "pnpm http-server dist -p 8080 -P http://localhost:8080? -c-1",
        "eslint": "eslint . --max-warnings=0 --cache --cache-location node_modules/.cache/eslint",
        "typecheck": "tsc"
    },
    "dependencies": {
        "@opentelemetry/api": "1.9.0",
        "@tanstack/react-query": "5.90.12",
        "@workleap/honeycomb": "workspace:*",
        "react": "19.2.1",
        "react-dom": "19.2.1",
        "react-router-dom": "7.10.1"
    },
    "devDependencies": {
        "@eslint/js": "9.39.1",
        "@rsbuild/core": "1.6.14",
        "@rspack/core": "1.6.7",
        "@types/react": "19.2.7",
        "@types/react-dom": "19.2.3",
        "@typescript-eslint/parser": "8.49.0",
        "@workleap/browserslist-config": "2.1.4",
        "@workleap/eslint-configs": "1.1.7",
        "@workleap/rsbuild-configs": "3.1.5",
        "@workleap/typescript-configs": "3.0.7",
        "browserslist": "4.28.1",
        "copyfiles": "2.4.1",
        "eslint": "9.39.1",
        "http-server": "14.1.1",
        "typescript": "5.9.3",
        "typescript-eslint": "8.49.0"
    }
}
